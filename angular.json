{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Bottesford": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/open-iconic-bootstrap.min.css",
              "src/assets/css/bootstrap.min.css",
              "node_modules/bootstrap/scss/bootstrap.scss",
              "src/assets/css/animate.css",
              "node_modules/owl.carousel/dist/assets/owl.carousel.css",  
              "node_modules/owl.carousel/dist/assets/owl.theme.default.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/aos.css",
              "src/assets/css/ionicons.min.css",
              "src/assets/css/bootstrap-datepicker.css",
              "src/assets/css/jquery.timepicker.css",
              "src/assets/css/flaticon.css",
              "src/assets/css/icomoon.css",
              "src/assets/css/style.css",
              "node_modules/@fortawesome/fontawesome-free-webfonts/css/fontawesome.css",
              "node_modules/@fortawesome/fontawesome-free-webfonts/css/fa-regular.css",
              "node_modules/@fortawesome/fontawesome-free-webfonts/css/fa-brands.css",
              "node_modules/@fortawesome/fontawesome-free-webfonts/css/fa-solid.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "src/assets/js/jquery-migrate-3.0.1.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/jquery.easing.1.3.js",
              "src/assets/js/jquery.waypoints.min.js",
              "src/assets/js/jquery.stellar.min.js",
              "node_modules/owl.carousel/dist/owl.carousel.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/aos.js",
              "src/assets/js/jquery.animateNumber.min.js",
              "src/assets/js/bootstrap-datepicker.js",
              "src/assets/js/scrollax.min.js",
              "src/assets/js/main.js",
              "node_modules/raphael/raphael.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "15mb",
                  "maximumError": "15mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "15mb",
                  "maximumError": "15mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Bottesford:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Bottesford:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Bottesford:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "Bottesford:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "Bottesford:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "Bottesford",
  "cli": {
    "analytics": "d3424b7d-966a-489c-9317-7593d4c526ba"
  }
}